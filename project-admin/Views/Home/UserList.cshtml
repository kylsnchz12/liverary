@model List<project_admin.Models.User>


<a asp-area="" asp-controller="Home" asp-action="Index">
    <h3>Back</h3>
</a>
<h1>Users</h1>
<table class="table">
    <thead>
        <tr>
            <th>UserID</th>
            <th>Username</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.UserID</td>
                <td>@user.Username</td>
                <td>@user.Password</td>
                <td><a href="/Home/View?userID=@user.UserID">View</a></td>
            </tr>
        }
    </tbody>
</table>